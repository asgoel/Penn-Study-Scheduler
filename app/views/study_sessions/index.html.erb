<h1>Your Study Sessions</h1>

<table>
  <tr>
    <th>Course</th>
    <th>Date</th>
    <th>Time</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
<% current_user.study_sessions.each do |study_session| %>
  <tr>
    <td><%= link_to study_session.course.abbrev, course_path(study_session.course) %></td>
    <td><%= study_session.date %></td>
    <td><%= study_session.time %></td>
    <td><%= study_session.location %></td>
  </tr>
<% end %>
</table>

<br />
